#include <iostream>
#include<string>
using namespace std;
int main() {
	string s;
	cin>>s;
	char a[100];
	int n= s.size();
	int flag=0;
	for(int i=0; i<n;i++){
	 a[i]=s[n-i-1];
	}
	for(int i=0;i<n;i++){
	if( a[i]!=s[i]){
		flag++;
	}
	}
	if(flag==0){
		cout<<"YES";
	}
	else{
		cout<<"NO";
	}
	return 0;
}
